<h2>Create-a-Heroine</h2>

<%=form_for(@heroine) do |f| %>
<%=f.label :secret_identity%>
<%=f.text_field :name %>
<br>
<br>
<%=f.label :heroine_name%>
<%=f.text_field :super_name %>
<br>
<br>
<%=f.label :super_power %>
<%=f.collection_select :power_id, Power.all, :id, :name, {:selected => 1}, {:multiple => true} %>
<br>
<br>
<br>
<%=f.submit "Make it so"%>
<%end%>

